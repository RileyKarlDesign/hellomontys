{%- assign collection = collections[section.settings.collection] -%}

<div class="block-wrapper">

  <div class="vendor-block">



    {% for i in collection.products %}
      <p>
        {{ i.vendor }}
      </p>
    {% endfor %}

  </div>
</div>

{% schema %}
  {
    "name": "labels",
    "settings": [
      {
        "type": "collection",
        "id": "collection",
        "label": "collection"
      }, {
        "type": "text",
        "id": "title_text",
        "label": "home-text"
      }, {
        "type": "text",
        "id": "sub_text",
        "label": "sub-text"
      }
    ],



    "presets": [
      {
        "name": "labels",
        "category": "Content"
      }
    ]
  }
{% endschema %}}